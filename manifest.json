{
    "manifest_version": 2,

    "name": "Bulldog Facebook",
    "description": "Adds information about Yale students on your facebook.",
    "version": "0.0",

    "content_scripts": [
        {
            "matches": ["http://www.facebook.com/*"],
            "css": ["injection/main.css"],
            "js": ["injection/jquery-2.1.4.min.js", "injection/main.js"]
        }
    ]
}
